{
  "name": "infinitum/laravel-repository",
  "description": "Laravel Repositories to abstract the database layer",
  "keywords": ["laravel", "repository", "eloquent", "model", "cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Khairil Anuar",
      "homepage": "https://infinitum.com.my",
      "email": "khairil@gmail.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/khairilanuar/laravel-repository",
  "support": {
    "email": "khairil@gmail.com",
    "issues": "https://github.com/khairilanuar/laravel-repository/issues",
    "wiki": "https://github.com/khairilanuar/laravel-repository",
    "source": "https://github.com/khairilanuar/laravel-repository",
    "docs": "https://github.com/khairilanuar/laravel-repository"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/khairilanuar/laravel-validation.git"
    }
  ],
  "require": {
    "illuminate/http": "~5.0|~6.0",
    "illuminate/config": "~5.0|~6.0",
    "illuminate/support": "~5.0|~6.0",
    "illuminate/database": "~5.0|~6.0",
    "illuminate/pagination": "~5.0|~6.0",
    "illuminate/console": "~5.0|~6.0",
    "illuminate/filesystem": "~5.0|~6.0",
    "infinitum/laravel-validation": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\Repository\\": "src/Prettus/Repository/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "suggest": {
    "league/fractal": "Required to use the Fractal Presenter (0.12.*).",
    "robclancy/presenter": "Required to use the Presenter Model (1.3.*)",
    "prettus/laravel-validation": "Required to provide easy validation with the repository (1.1.*)"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prettus\\Repository\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
